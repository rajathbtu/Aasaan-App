{
  "expo": {
    "name": "Aasaan",
    "slug": "aasaan-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "in.aasaanapp.aasaan",
      "buildNumber": "1",
      "supportsTablet": true,
      "googleServicesFile": "./ios/GoogleService-Info.plist",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Aasaan uses your location to show nearby services.",
        "NSUserTrackingUsageDescription": "We use notifications to alert you about updates to your requests.",
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "package": "in.aasaanapp.aasaan",
      "versionCode": 1,
      "googleServicesFile": "./android/app/google-services.json",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "POST_NOTIFICATIONS",
        "WAKE_LOCK",
        "RECEIVE_BOOT_COMPLETED"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "scheme": "aasaan",
    "plugins": [
      [
        "@react-native-firebase/app",
        {
          "android": {
            "googleServicesFile": "./android/app/google-services.json"
          },
          "ios": {
            "googleServicesFile": "./ios/GoogleService-Info.plist"
          }
        }
      ],
      "expo-dev-client",
      [
        "expo-notifications",
        {
          "icon": "./assets/logo.png",
          "color": "#4F46E5",
          "mode": "production",
          "androidMode": "exact",
          "androidCollapsedTitle": "#{unread_notifications} new interactions"
        }
      ],
      "expo-secure-store",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "30a58cf2-a668-45d2-81a8-130261a7a756"
      }
    },
    "owner": "aasaan-app"
  }
}
